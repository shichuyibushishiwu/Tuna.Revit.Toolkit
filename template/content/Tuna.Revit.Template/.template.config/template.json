{
  "$schema": "http://json.schemastore.org/template",
  "author": "Shiwu",
  "classifications": [ "Tuna", "Windows", "Revit", "Add-In", "BIM", "WPF" ],
  "identity": "Tuna Revit Application",
  "name": "Tuna Revit Application",
  "description": "A Tuna Revit add-in project template with multi-version selection",
  "shortName": "tuna-revit-app",
  "icon": "Tuna.ico",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sources": [
    {
      "modifiers": [
        {
          "exclude": [
            "[Content_Types].xml",
            "_rels/**/*",
            "package/**/*",
            "*.nupkg",
            "*.snupkg",
            "*.nuspec"
          ]
        }
      ]
    }
  ],
  "sourceName": "Tuna.Revit.Template",
  "symbols": {
    "appName": {
      "type": "parameter",
      "datatype": "string",
      "displayName": "应用名称",
      "description": "用于 程序在 Revit 中 的显示名称",
      "defaultValue": "TunaApp",
      "replaces": "$appName$"
    },
    "rvt16": {
      "type": "parameter",
      "datatype": "bool",
      "displayName": "支持 Revit 2016",
      "defaultValue": false
    },
    "rvt17": {
      "type": "parameter",
      "datatype": "bool",
      "displayName": "支持 Revit 2017",
      "defaultValue": false
    },
    "rvt18": {
      "type": "parameter",
      "datatype": "bool",
      "displayName": "支持 Revit 2018",
      "defaultValue": false
    },
    "rvt19": {
      "type": "parameter",
      "datatype": "bool",
      "displayName": "支持 Revit 2019",
      "defaultValue": false
    },
    "rvt20": {
      "type": "parameter",
      "datatype": "bool",
      "displayName": "支持 Revit 2020",
      "defaultValue": true
    },
    "rvt21": {
      "type": "parameter",
      "datatype": "bool",
      "displayName": "支持 Revit 2021",
      "defaultValue": true
    },
    "rvt22": {
      "type": "parameter",
      "datatype": "bool",
      "displayName": "支持 Revit 2022",
      "defaultValue": true
    },
    "rvt23": {
      "type": "parameter",
      "datatype": "bool",
      "displayName": "支持 Revit 2023",
      "defaultValue": true
    },
    "rvt24": {
      "type": "parameter",
      "datatype": "bool",
      "displayName": "支持 Revit 2024",
      "defaultValue": true
    },
    "rvt25": {
      "type": "parameter",
      "datatype": "bool",
      "displayName": "支持 Revit 2025",
      "defaultValue": true
    },
    "rvt26": {
      "type": "parameter",
      "datatype": "bool",
      "displayName": "支持 Revit 2026",
      "defaultValue": true
    },
    "clientId": {
      "type": "generated",
      "generator": "guid",
      "replaces": "$guid1$"
    }
  }
}
