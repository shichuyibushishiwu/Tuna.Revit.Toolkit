	<PropertyGroup Condition="'$(Configuration.StartsWith(Rvt_26))'">
		<RvtVersion>2026.0</RvtVersion>
		<DefineConstants>Rvt_26</DefineConstants>
		<TargetFramework>net8.0-windows</TargetFramework>
	</PropertyGroup>

	<PropertyGroup>
		<Version>$(RvtVersion).$(TunaVer)</Version>
		<PackageTags>Revit;ITuna</PackageTags>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Rvt_16_Release|AnyCPU'">
